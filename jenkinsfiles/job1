pipeline {
    agent any
    stages {
        stage('Clean') {
            steps {
                deleteDir()
            }
        }
        stage('Build') {
            steps {
                sh '''
                    i=1
                    while [ $i -le 100 ]
                    do
                        echo $i
                        i=$(($i+1))
                    done
                    '''
            }
        }
    }
}
